@{
}
<h1>Search Results</h1>
@foreach (var job in ViewBag.Jobs)
{
    <table class="job-listing">
        <tr>
            <th>ID</th>
            <td>@job.Id</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>@job.Name</td>
        </tr>
        <tr>
            <th>Employer</th>
            <td>@job.Location</td>
        </tr>
        <tr>
            <th>Position Type</th>
            <td>@job.PositionType</td>
        </tr>
        <tr>
            <th>Skill</th>
            <td>@job.CoreCompetency</td>
        </tr>
    </table>
}